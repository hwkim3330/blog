[global]
# P2P Delay Mechanism
delay_mechanism     P2P

# Layer 2 Ethernet transport (VelocityDRIVE-SP는 L2 프레임 사용)
network_transport   IEEE_802_3

# 하드웨어 타임스탬프 사용
time_stamping       HARDWARE

# Transparent Clock 모드로 설정
tc_type             P2P

# 로깅 수준 설정 (선택)
logging_level       6

[enp7s0]
EOF

